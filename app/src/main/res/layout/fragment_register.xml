<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context="com.iyoa.cleanaddis.controller.account.RegisterFragment">
    <View
            android:id="@+id/divider9"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
            tools:layout_editor_absoluteY="365dp" tools:layout_editor_absoluteX="0dp"/>
    <com.google.android.material.button.MaterialButton android:layout_width="0dp"
                                                       android:layout_height="wrap_content"
                                                       app:layout_constraintTop_toBottomOf="@+id/materialButton"
                                                       app:layout_constraintEnd_toEndOf="parent"
                                                       android:layout_marginEnd="8dp"
                                                       app:layout_constraintStart_toStartOf="parent"
                                                       android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                                                       android:background="#FFB300"
                                                       android:text="@string/common_signin_button_text"
                                                       android:textColor="#00897B"
                                                       android:id="@+id/account_signup_button"/>
    <com.google.android.material.button.MaterialButton android:layout_width="0dp"
                                                       android:layout_height="wrap_content"
                                                       app:layout_constraintBottom_toTopOf="@+id/divider9"
                                                       android:id="@+id/materialButton"
                                                       app:layout_constraintEnd_toEndOf="parent"
                                                       android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                                                       app:layout_constraintStart_toStartOf="parent"
                                                       android:layout_marginLeft="8dp"
                                                       android:layout_marginStart="8dp"
                                                       android:layout_marginBottom="8dp" android:background="#00897B"
                                                       android:text="Sign up" android:textColor="#FFFFFF"/>


</androidx.constraintlayout.widget.ConstraintLayout>
